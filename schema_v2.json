{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Paper",
  "required": [
    "reference",
    "data",
    "preprocessing",
    "analysis_method",
    "evaluation",
    "result"
  ],
  "additionalProperties": false,
  "definitions": {
    "feature": {
      "title": "feature",
      "type": [
        "object",
        "string",
        "number",
        "boolean"
      ],
      "additionalProperties": {
        "type": "array",
        "items": {
          "$ref": "#/definitions/feature"
        },
        "minItems": 1
      }
    },
    "key": {
      "type": "array",
      "title": "key_features",
      "items": {
        "$ref": "#/definitions/feature"
      }
    },
    "features": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "$ref": "#/definitions/feature"
      }
    },
    "tool": {
      "title": "tool",
      "type": "object",
      "properties": {
        "tool": {
          "title": "tool",
          "type": "string",
          "minLength": 1
        },
        "detail": {
          "title": "detail",
          "type": "string",
          "minLength": 1
        },
        "availability": {
          "title": "availability",
          "type": "string",
          "minLength": 1,
          "oneOf": [
            {
              "title": "default values",
              "enum": [
                "public",
                "private",
                "public_on_demand",
                "commercial"
              ]
            },
            {
              "$ref": "#/definitions/other"
            },
            {
              "$ref": "#/definitions/missing"
            }
          ]
        }
      },
      "required": [
        "tool",
        "availability"
      ]
    },
    "selection": {
      "title": "selection",
      "type": "string",
      "minLength": 1,
      "oneOf": [
        {
          "title": "default values",
          "enum": [
            "in_dataset",
            "feature_selection",
            "study_based",
            "tool_based",
            "expert_knowledge"
          ]
        },
        {
          "$ref": "#/definitions/other"
        },
        {
          "$ref": "#/definitions/missing"
        }
      ]
    },
    "role": {
      "title": "role",
      "type": "string",
      "minLength": 1,
      "enum": [
        "main",
        "competitor",
        "validation"
      ]
    },
    "goal": {
      "title": "main_goal",
      "type": "string",
      "minLength": 1,
      "oneOf": [
        {
          "title": "default values",
          "enum": [
            "anomaly_detection",
            "application_classification",
            "application_protocol_detection",
            "attack_classification",
            "botnet_detection",
            "classification_for_qos",
            "classification_of_encrypted_traffic",
            "ddos_detection",
            "dos_detection",
            "http_intrusion_detection",
            "network_properties_monitoring",
            "p2p_botnet_detection",
            "p2p_traffic_classification",
            "probe_detection",
            "remote_to_local_detection",
            "specific_malware_detection",
            "traffic_classification",
            "traffic_rate_prediction",
            "traffic_visualization",
            "user_to_root_detection"
          ]
        },
        {
          "$ref": "#/definitions/other"
        },
        {
          "$ref": "#/definitions/missing"
        }
      ]
    },
    "time": {
      "type": [
        "number",
        "string"
      ],
      "oneOf": [
        {
          "type": "string",
          "minLength": 1,
          "enum": [
            "missing"
          ]
        },
        {
          "type": "number"
        }
      ],
      "title": "time"
    },
    "other": {
      "type": "string",
      "minLength": 1,
      "pattern": "^_[A-Za-z0-9-_]+",
      "title": "other"
    },
    "missing": {
      "title": "missing",
      "type": "string",
      "minLength": 1,
      "enum": [
        "missing"
      ]
    }
  },
  "properties": {
    "reference": {
      "title": "reference",
      "type": "object",
      "properties": {
        "title": {
          "title": "title",
          "type": "string",
          "minLength": 1
        },
        "1st_author": {
          "title": "1st_author",
          "type": "string",
          "minLength": 1
        },
        "authors": {
          "title": "authors",
          "type": "array",
          "items": {
            "type": "string",
            "minLength": 1,
            "title": "author"
          }
        },
        "publication_name": {
          "title": "publication_name",
          "type": "string",
          "minLength": 1
        },
        "publication_type": {
          "title": "publication_type",
          "type": "string",
          "minLength": 1,
          "oneOf": [
            {
              "enum": [
                "peer_reviewed_journal",
                "peer_reviewed_conference",
                "technical_report",
                "arxiv"
              ],
              "title": "default values"
            },
            {
              "$ref": "#/definitions/other"
            }
          ]
        },
        "year": {
          "title": "year",
          "type": "integer",
          "minimum": 1900
        },
        "organization_publishers": {
          "title": "organization_publishers",
          "type": "array",
          "minItems": 1,
          "items": {
            "title": "organization_publisher",
            "type": "string",
            "minLength": 1,
            "oneOf": [
              {
                "title": "default values",
                "enum": [
                  "ieee",
                  "elsevier",
                  "acm",
                  "springer",
                  "wiley",
                  "taylor_&_francis",
                  "mdpi"
                ]
              },
              {
                "$ref": "#/definitions/other"
              }
            ]
          }
        },
        "pages_number_of": {
          "title": "pages_number_of",
          "type": "integer",
          "minimum": 0
        },
        "bibtex": {
          "type": "object",
          "title": "bibtex",
          "properties": {
            "type": {
              "title": "type",
              "type": "string"
            },
            "volume": {
              "title": "volume",
              "type": "string"
            },
            "issue": {
              "type": "string",
              "title": "issue"
            },
            "pages": {
              "title": "pages",
              "type": "string"
            }
          },
          "additionalProperties": {
            "type": "string"
          },
          "required": [
            "type", "volume", "issue", "pages"
          ]
        },
        "access_open": {
          "title": "access_open",
          "type": "boolean"
        },
        "curated_by": {
          "title": "curated_by",
          "type": "string",
          "minLength": 1
        },
        "curated_last_revision": {
          "title": "curated_last_revision",
          "type": "string",
          "minLength": 1,
          "pattern": "^[0-9]{2}-[0-9]{2}-[0-9]{4}$",
          "description": "format: dd-mm-yyyy"
        },
        "curated_revision_number": {
          "title": "curated_revision_number",
          "type": "integer",
          "minimum": 1
        }
      },
      "required": [
        "title",
        "1st_author",
        "authors",
        "publication_name",
        "publication_type",
        "year",
        "bibtex",
        "curated_by",
        "curated_last_revision",
        "curated_revision_number"
      ]
    },
    "data": {
      "title": "data",
      "type": "object",
      "properties": {
        "datasets": {
          "type": "array",
          "title": "datasets",
          "items": {
            "title": "dataset",
            "type": "object",
            "properties": {
              "dataset_name": {
                "title": "dataset_name",
                "type": "string",
                "minLength": 1
              },
              "availability": {
                "title": "availability",
                "type": "string",
                "minLength": 1,
                "oneOf": [
                  {
                    "enum": [
                      "public",
                      "public_on_demand",
                      "private",
                      "lost_source"
                    ],
                    "title": "default values"
                  },
                  {
                    "$ref": "#/definitions/other"
                  },
                  {
                    "$ref": "#/definitions/missing"
                  }
                ]
              },
              "format": {
                "title": "format",
                "type": "string",
                "minLength": 1,
                "oneOf": [
                  {
                    "title": "default values",
                    "enum": [
                      "packet",
                      "flow"
                    ]
                  },
                  {
                    "$ref": "#/definitions/other"
                  },
                  {
                    "$ref": "#/definitions/missing"
                  }
                ]
              },
              "types": {
                "title": "types",
                "type": "array",
                "items": {
                  "title": "type",
                  "type": "string",
                  "oneOf": [
                    {
                      "enum": [
                        "ip",
                        "ipv4",
                        "ipv6",
                        "tcp",
                        "http",
                        "udp",
                        "icmp",
                        "dns"
                      ],
                      "title": "default values"
                    },
                    {
                      "$ref": "#/definitions/other"
                    },
                    {
                      "$ref": "#/definitions/missing"
                    }
                  ]
                }
              },
              "generation": {
                "title": "generation",
                "type": "string",
                "minLength": 1,
                "oneOf": [
                  {
                    "title": "default values",
                    "enum": [
                      "captured",
                      "synthetic",
                      "altered_captured",
                      "mixed"
                    ]
                  },
                  {
                    "$ref": "#/definitions/other"
                  },
                  {
                    "$ref": "#/definitions/missing"
                  }
                ]
              },
              "generation_year": {
                "title": "generation_year",
                "oneOf": [
                  {
                    "title": "year",
                    "type": "integer",
                    "minimum": 1900
                  },
                  {
                    "title": "years",
                    "type": "array",
                    "items": {
                      "type": "integer",
                      "title": "year",
                      "minimum": 1900
                    }
                  },
                  {
                    "$ref": "#/definitions/missing"
                  }
                ]
              },
              "covered_period": {
                "title": "covered_period",
                "type": "string",
                "minLength": 1,
                "oneOf": [
                  {
                    "title": "default values",
                    "enum": [
                      "minutes",
                      "hours",
                      "days",
                      "weeks",
                      "years"
                    ]
                  },
                  {
                    "$ref": "#/definitions/other"
                  },
                  {
                    "$ref": "#/definitions/missing"
                  }
                ]
              },
              "details": {
                "title": "details",
                "type": "array",
                "items": {
                  "title": "detail",
                  "type": "string",
                  "minLength": 1,
                  "oneOf": [
                    {
                      "title": "default values",
                      "enum": [
                        "raw",
                        "preprocessed",
                        "no_payload"
                      ]
                    },
                    {
                      "$ref": "#/definitions/other"
                    }
                  ]
                }
              },
              "subsets": {
                "title": "subsets",
                "type": "array",
                "items": {
                  "title": "subset",
                  "type": "string",
                  "minLength": 1
                }
              }
            },
            "required": [
              "dataset_name",
              "generation_year",
              "subsets"
            ]
          }
        }
      }
    },
    "preprocessing": {
      "title": "preprocessing",
      "type": "object",
      "properties": {
        "performed_feature_selection": {
          "title": "performed_feature_selection",
          "type": "boolean"
        },
        "packet_analysis_oriented": {
          "title": "packet_analysis_oriented",
          "type": "boolean"
        },
        "flow_analysis_oriented": {
          "title": "flow_analysis_oriented",
          "type": "boolean"
        },
        "flow_aggregation_analysis_oriented": {
          "title": "flow_aggregation_analysis_oriented",
          "type": "boolean"
        },
        "tools": {
          "title": "tools",
          "type": "array",
          "items": {
            "$ref": "#/definitions/tool"
          }
        },
        "normalization_type": {
          "title": "normalization_type",
          "type": "string",
          "minLength": 1,
          "oneOf": [
            {
              "title": "default values",
              "enum": [
                "no",
                "not_applicable",
                "range",
                "zscore",
                "decimal_scaling",
                "quartile"
              ]
            },
            {
              "$ref": "#/definitions/other"
            },
            {
              "$ref": "#/definitions/missing"
            }
          ]
        },
        "transformations": {
          "title": "transformations",
          "type": "array",
          "items": {
            "title": "transformation",
            "type": "string",
            "minLength": 1,
            "oneOf": [
              {
                "title": "default values",
                "enum": [
                  "no",
                  "sampling",
                  "filtering",
                  "log",
                  "map",
                  "graph",
                  "feature_aggregation",
                  "flow_extraction",
                  "entropy",
                  "time_series",
                  "feature_operation",
                  "class_separation"
                ]
              },
              {
                "$ref": "#/definitions/other"
              },
              {
                "$ref": "#/definitions/missing"
              }
            ]
          }
        },
        "final_data_format": {
          "title": "final_data_format",
          "type": "string",
          "minLength": 1,
          "oneOf": [
            {
              "title": "default values",
              "enum": [
                "numerical_vectors",
                "nominal_vectors",
                "mixed_vectors",
                "strings",
                "time_series"
              ]
            },
            {
              "$ref": "#/definitions/other"
            },
            {
              "$ref": "#/definitions/missing"
            }
          ]
        },
        "feature_selections": {
          "title": "feature_selections",
          "type": "array",
          "items": {
            "title": "feature_selection",
            "type": "object",
            "properties": {
              "name": {
                "title": "name",
                "type": "string",
                "minLength": 1
              },
              "type": {
                "title": "type",
                "type": "string",
                "minLength": 1,
                "oneOf": [
                  {
                    "title": "default values",
                    "enum": [
                      "wrapper",
                      "filter",
                      "hybrid",
                      "nest",
                      "feature_reduction"
                    ]
                  },
                  {
                    "$ref": "#/definitions/other"
                  },
                  {
                    "$ref": "#/definitions/missing"
                  }
                ]
              },
              "classifier": {
                "title": "classifier",
                "type": "string",
                "minLength": 1
              },
              "role": {
                "$ref": "#/definitions/role"
              }
            },
            "required": [
              "name"
            ]
          }
        },
        "packets": {
          "title": "packets",
          "type": "array",
          "items": {
            "title": "packet",
            "type": "object",
            "properties": {
              "selection": {
                "$ref": "#/definitions/selection"
              },
              "role": {
                "$ref": "#/definitions/role"
              },
              "main_goal": {
                "$ref": "#/definitions/goal"
              },
              "features": {
                "$ref": "#/definitions/features"
              }
            }
          }
        },
        "flows": {
          "title": "flows",
          "type": "array",
          "items": {
            "title": "flow",
            "type": "object",
            "properties": {
              "selection": {
                "$ref": "#/definitions/selection"
              },
              "role": {
                "$ref": "#/definitions/role"
              },
              "main_goal": {
                "$ref": "#/definitions/goal"
              },
              "active_timeout": {
                "title": "active_timeout",
                "oneOf": [
                  {
                    "$ref": "#/definitions/time"
                  },
                  {
                    "type": "array",
                    "items": {
                      "$ref": "#/definitions/time"
                    },
                    "title": "times"
                  }
                ]
              },
              "idle_timeout": {
                "title": "idle_timeout",
                "oneOf": [
                  {
                    "$ref": "#/definitions/time"
                  },
                  {
                    "type": "array",
                    "items": {
                      "$ref": "#/definitions/time"
                    },
                    "title": "times"
                  }
                ]
              },
              "bidirectional": {
                "title": "bidirectional",
                "type": "boolean"
              },
              "features": {
                "$ref": "#/definitions/features"
              },
              "key_features": {
                "$ref": "#/definitions/key"
              }
            }
          }
        },
        "flow_aggregations": {
          "title": "flow_aggregations",
          "type": "array",
          "items": {
            "title": "flow_aggregation",
            "type": "object",
            "properties": {
              "selection": {
                "$ref": "#/definitions/selection"
              },
              "role": {
                "$ref": "#/definitions/role"
              },
              "main_goal": {
                "$ref": "#/definitions/goal"
              },
              "active_timeout": {
                "title": "active_timeout",
                "oneOf": [
                  {
                    "$ref": "#/definitions/time"
                  },
                  {
                    "type": "array",
                    "items": {
                      "$ref": "#/definitions/time"
                    },
                    "title": "times"
                  }
                ]
              },
              "features": {
                "$ref": "#/definitions/features"
              },
              "key_features": {
                "$ref": "#/definitions/key"
              }
            }
          }
        }
      },
      "required": [
        "performed_feature_selection",
        "packet_analysis_oriented",
        "flow_analysis_oriented",
        "flow_aggregation_analysis_oriented",
        "tools",
        "normalization_type",
        "transformations",
        "final_data_format"
      ]
    },
    "analysis_method": {
      "title": "analysis_method",
      "type": "object",
      "properties": {
        "supervised_learning": {
          "title": "supervised_learning",
          "type": "boolean"
        },
        "unsupervised_learning": {
          "title": "unsupervised_learning",
          "type": "boolean"
        },
        "semisupervised_learning": {
          "title": "semisupervised_learning",
          "type": "boolean"
        },
        "anomaly_detection": {
          "title": "anomaly_detection",
          "type": "boolean"
        },
        "tools": {
          "title": "tools",
          "type": "array",
          "items": {
            "$ref": "#/definitions/tool"
          }
        },
        "algorithms": {
          "title": "algorithms",
          "type": "array",
          "items": {
            "title": "algorithm",
            "type": "object",
            "properties": {
              "name": {
                "title": "name",
                "type": "string",
                "minLength": 1
              },
              "subname": {
                "title": "subname",
                "type": "string",
                "minLength": 1
              },
              "learning": {
                "title": "learning",
                "type": "string",
                "minLength": 1,
                "oneOf": [
                  {
                    "title": "default values",
                    "enum": [
                      "supervised",
                      "unsupervised",
                      "semisupervised",
                      "statistics/model_fit",
                      "nest",
                      "no"
                    ]
                  },
                  {
                    "$ref": "#/definitions/other"
                  },
                  {
                    "$ref": "#/definitions/missing"
                  }
                ]
              },
              "role": {
                "$ref": "#/definitions/role"
              },
              "type": {
                "title": "type",
                "type": "string",
                "minLength": 1,
                "oneOf": [
                  {
                    "title": "default values",
                    "enum": [
                      "classification",
                      "regression",
                      "clustering",
                      "heuristics",
                      "anomaly_detection",
                      "statistics",
                      "text_matching"
                    ]
                  },
                  {
                    "$ref": "#/definitions/other"
                  },
                  {
                    "$ref": "#/definitions/missing"
                  }
                ]
              },
              "metric/decision_criteria": {
                "title": "metric/decision_criteria",
                "type": "string",
                "minLength": 1,
                "oneOf": [
                  {
                    "title": "default values",
                    "enum": [
                      "error/fitting_function",
                      "euclidean",
                      "mutual_information",
                      "correlation",
                      "jaccard",
                      "mahalanobis",
                      "hamming",
                      "exact_matching",
                      "manhattan",
                      "probabilistic",
                      "vote",
                      "no"
                    ]
                  },
                  {
                    "$ref": "#/definitions/other"
                  },
                  {
                    "$ref": "#/definitions/missing"
                  }
                ]
              },
              "tools": {
                "title": "tools",
                "type": "array",
                "items": {
                  "$ref": "#/definitions/tool"
                }
              },
              "source": {
                "title": "source",
                "type": "string",
                "minLength": 1,
                "oneOf": [
                  {
                    "title": "default values",
                    "enum": [
                      "own_proposed",
                      "own_referenced",
                      "referenced"
                    ]
                  },
                  {
                    "$ref": "#/definitions/other"
                  },
                  {
                    "$ref": "#/definitions/missing"
                  }
                ]
              },
              "parameters_provided": {
                "title": "parameters_provided",
                "oneOf": [
                  {
                    "title": "boolean",
                    "type": "boolean"
                  },
                  {
                    "title": "partially",
                    "enum": [
                      "partially"
                    ]
                  }
                ]
              }
            },
            "required": [
              "name"
            ]
          }
        }
      },
      "required": [
        "supervised_learning",
        "unsupervised_learning",
        "semisupervised_learning",
        "anomaly_detection",
        "tools"
      ]
    },
    "evaluation": {
      "title": "evaluation",
      "type": "object",
      "properties": {
        "algorithm_comparison": {
          "title": "algorithm_comparison",
          "type": "boolean"
        },
        "internal_validation": {
          "title": "internal_validation",
          "type": "boolean"
        },
        "external_validation": {
          "title": "external_validation",
          "type": "boolean"
        },
        "dpi-based_validation": {
          "title": "dpi-based_validation",
          "type": "boolean"
        },
        "port-based_validation": {
          "title": "port-based_validation",
          "type": "boolean"
        },
        "pre-knowledge-based_validation": {
          "title": "pre-knowledge-based_validation",
          "type": "boolean"
        },
        "manual_verification": {
          "title": "manual_verification",
          "type": "boolean"
        },
        "implementation_in_real_scenario": {
          "title": "implementation_in_real_scenario",
          "type": "boolean"
        },
        "train_test_separation": {
          "title": "train_test_separation",
          "type": "boolean"
        },
        "methods": {
          "title": "methods",
          "type": "array",
          "items": {
            "title": "evaluation_method",
            "type": "object",
            "properties": {
              "name": {
                "title": "name",
                "type": "string",
                "minLength": 1
              },
              "type": {
                "title": "type",
                "type": "string",
                "minLength": 1,
                "oneOf": [
                  {
                    "title": "default values",
                    "enum": [
                      "external",
                      "internal",
                      "external_and_internal",
                      "nest"
                    ]
                  },
                  {
                    "$ref": "#/definitions/other"
                  },
                  {
                    "$ref": "#/definitions/missing"
                  }
                ]
              },
              "metrics": {
                "title": "metrics",
                "type": "array",
                "items": {
                  "title": "metric",
